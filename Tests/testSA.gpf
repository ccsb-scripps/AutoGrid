npts 4 4 4                           # num.grid points in xyz
parameter_file parm_data.dat         # force field default parm file
gridfld testSA.maps.fld                # grid_data_file
spacing 1.000                        # spacing(A)
ligand_types SA                      # ligand atom type
receptor_types C A N NA OA SA HD     # receptor atom types
receptor hsg1.pdbqt                  # macromolecule
gridcenter 2.5 6.5 -7.5              # xyz-coordinates or auto
ligand testSA.pdbqt                  # ligand to be evaluated  NB: vol solpar
sol_par  N 22.44930 -0.00162
sol_par OA 17.15728 -0.00251
sol_par HD  0.00000  0.00051
sol_par  C 33.51032 -0.00143
sol_par NA 22.44930 -0.00162
sol_par  A 33.51032 -0.00052
sol_par SA 33.51032 -0.00214
smooth 0.5                           # store minimum energy w/in rad(A)
map testSA.SA.map                       # atom-specific affinity map
nbp_r_eps SA  C  4.00 0.0257202 12  6   # SA-C lj
nbp_r_eps SA  A  4.00 0.0257202 12  6   # SA-A lj
nbp_r_eps SA  N  3.75 0.0265667 12  6   # SA-N lj
nbp_r_eps SA NA  3.75 0.0265667 12  6   # SA-NA lj
nbp_r_eps SA OA  3.60 0.0297000 12  6   # SA-OA lj
nbp_r_eps SA SA  4.00 0.2970000 12  6   # SA-SA lj
nbp_r_eps SA HD  2.50 0.0656000 12 10   # SA-HD lj
elecmap testSA.e.map                # electrostatic potential map
dsolvmap testSA.d.map
dielectric -0.1146                   # <0, distance-dep.diel;>0, constant
