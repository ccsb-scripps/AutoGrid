npts 4 4 4                           # num.grid points in xyz
parameter_file parm_data.dat         # force field default parm file
gridfld testS.maps.fld                # grid_data_file
spacing 1.000                        # spacing(A)
ligand_types S                       # ligand atom type
receptor_types C A N NA OA SA HD     # receptor atom types
receptor hsg1.pdbqt                  # macromolecule
gridcenter 2.5 6.5 -7.5              # xyz-coordinates or auto
ligand testS.pdbqt                   # ligand to be evaluated  NB: vol solpar
sol_par  N 22.44930 -0.00162
sol_par OA 17.15728 -0.00251
sol_par HD  0.00000  0.00051
sol_par  C 33.51032 -0.00143
sol_par NA 22.44930 -0.00162
sol_par  A 33.51032 -0.00052
sol_par SA 33.51032 -0.00214
sol_par  S 33.51032 -0.00214
smooth 0.5                           # store minimum energy w/in rad(A)
map testS.S.map                       # atom-specific affinity map
nbp_r_eps  S  C  4.00 0.0257202 12  6   # S-C lj
nbp_r_eps  S  A  4.00 0.0257202 12  6   # S-A lj
nbp_r_eps  S  N  3.75 0.0265667 12  6   # S-N lj
nbp_r_eps  S NA  3.75 0.0265667 12  6   # S-NA lj
nbp_r_eps  S OA  3.60 0.0297000 12  6   # S-OA lj
nbp_r_eps  S SA  4.00 0.2970000 12  6   # S-SA lj
nbp_r_eps  S HD  3.00 0.0093852 12  6   # S-HD lj
elecmap testS.e.map                # electrostatic potential map
dsolvmap testS.d.map
dielectric -0.1146                   # <0, distance-dep.diel;>0, constant
