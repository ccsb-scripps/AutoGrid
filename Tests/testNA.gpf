npts 4 4 4                           # num.grid points in xyz
parameter_file parm_data.dat         # force field default parm file
gridfld testNA.maps.fld                # grid_data_file
spacing 1.000                        # spacing(A)
ligand_types NA                       # ligand atom type
receptor_types C A N NA OA SA HD     # receptor atom types
receptor hsg1.pdbqt                  # macromolecule
gridcenter 2.5 6.5 -7.5              # xyz-coordinates or auto
ligand testNA.pdbqt                  # ligand to be evaluated  NB: vol solpar
sol_par  N 22.44930 -0.00162
sol_par OA 17.15728 -0.00251
sol_par HD  0.00000  0.00051
sol_par  C 33.51032 -0.00143
sol_par NA 22.44930 -0.00162
sol_par  A 33.51032 -0.00052
sol_par SA 33.51032 -0.00214
smooth 0.5                           # store minimum energy w/in rad(A)
map testNA.NA.map                       # atom-specific affinity map
nbp_r_eps NA  C  3.75 0.0230026 12  6   # NA-C lj
nbp_r_eps NA  A  3.75 0.0230026 12  6   # NA-A lj
nbp_r_eps NA  N  3.50 0.0237600 12  6   # NA-N lj
nbp_r_eps NA NA  3.50 0.0237600 12  6   # NA-NA lj
nbp_r_eps NA OA  3.35 0.0265667 12  6   # NA-OA lj
nbp_r_eps NA SA  3.75 0.0265667 12  6   # NA-SA lj
nbp_r_eps NA HD  1.90 0.3280000 12 10   # NA-HD hb
elecmap testNA.e.map                # electrostatic potential map
dsolvmap testNA.d.map
dielectric -0.1146                   # <0, distance-dep.diel;>0, constant
